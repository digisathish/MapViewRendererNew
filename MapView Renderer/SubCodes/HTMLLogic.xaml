    </Sequence.Variables>
    <Assign DisplayName="Initial Value for FlagVar" sap2010:WorkflowViewState.IdRef="Assign_14">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Boolean">[FlagVar]</OutArgument>
      </Assign.To>
      <Assign.Value>
